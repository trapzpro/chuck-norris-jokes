{
    "name": "trapzpro/chuck-norris-jokes",
    "description": "Create random Chuck Norris jokes.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Blake",
            "email": "Blake.Weber@windstream.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Trapzpro\\ChuckNorrisJokes\\":"src/"
        }
    }, 
    "autoload-dev": {
        "psr-4": {
            "Trapzpro\\ChuckNorrisJokes\\Tests\\":"test"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.7"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trapzpro\\ChuckNorrisJokes\\ChuckNorrisJokesServiceProvider"
            ],
            "aliases": {
                "ChuckNorris": "Trapzpro\\ChuckNorrisJokes\\Facades\\ChuckNorris"
            }
        }
    }
}
